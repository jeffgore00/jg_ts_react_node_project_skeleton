{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": [
      "node",
      "express",
      "react",
      "react-dom",
      "styled-components",
      "jest",
      "@wdio/sync"
    ]
  },
  "include": ["src", "test-browser", "test-api", "test-utils", "."],
  "exclude": ["wdio.conf.ts"]
}
